
session=0
window=${session}
pane=${window}.0
pane2=${window}.1
tmux send-keys -t "$pane" C-z './PinBoardFiles/runboard.sh' Enter
tmux select-pane -t "$pane"
tmux select-window -t "$window"
tmux send-keys -t "$pane2" C-z './PinBoardFiles/pinwall.sh' Enter
tmux select-pane -t "$pane2"
tmux select-window -t "$window2"
tmux attach-session -t "$session"





